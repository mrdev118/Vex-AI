{
  "bot": {
    "prefix": "!",
    "name": "AURABOT"
  },
  "paths": {
    "appstate": "./appstate.json",
    "commands": "./commands"
  },
  "api": {
    "forceLogin": true,
    "listenEvents": true,
    "logLevel": "error",
    "selfListen": false
  },
  "login": {
    "email": "YOUR_EMAIL",
    "password": "YOUR_PASSWORD"
  },
  "externalApi": {
    "url": "https://minhdong.site",
    "key": "YOUR_API_KEY_HERE"
  },
  "logger": {
    "level": "info",
    "enableColors": true,
    "enableTimestamp": true
  },
  "permissions": {
    "owner": "YOUR_OWNER_ID",
    "admins": []
  }
}
